{
    "manifest_version": 3,
    "name": "Ask Your Bookmarks",
    "version": "1",
    "background": {
        "scripts": ["browser-polyfill.js", "background.js"],
        "type": "module"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },

    

    "permissions": [
        "bookmarks"
    ]

    
}